<% @user.each do |user| %>
  <tr>
    <td class="hidden-sm hidden-xs">
      <p><%= user.id %></p>
    </td>
    <td>
      <p><%= user.first_name.capitalize if user.first_name? %> <%= user.last_name.capitalize if user.last_name? %></p>
    </td>
    <td class="hidden-sm hidden-xs">
      <p><%= user.email %></p>
    </td>
    <td class="hidden-sm hidden-xs">
      <p><%= user.try(:type) %></p>
    </td>
    <td class="hidden-sm hidden-xs">
      <p><%= user.title if user.title? %></p>
    </td>
    <td class="hidden-sm hidden-xs">
      <p><%= user.customer_name if user.customer_name? %></p>
    </td>
    <td class="hidden-sm hidden-xs">
      <p><%= user.customer_id if user.customer_id? %></p>
    </td>
    <td>
      <p><%= user.created_at.strftime('%b-%d-%y')  %></p>
    </td>
    <!-- < if  current_user.try(:type) == 'SuperUser'%> -->
      <!-- <td>
        <= link_to "", edit_user_path(user), id: "edit_#{user.id}", class: 'glyphicon glyphicon-pencil index-icons' %>
      </td> -->
    <!-- < end %> -->
    <% if current_user.try(:type) == 'SuperUser' %>
      <td>
        <!-- < if current_user.id == group.user_id %> -->
          <%= link_to "", user_path(user), method: :delete, id: "delete_ink_#{user.id}_from_index", class: 'glyphicon glyphicon glyphicon-trash index-icons', data: {confirm: 'Are you sure you want to delete this user?'} %>
        <!-- < end %> -->
      </td>
    <% end %>
  </tr>
<% end %>

